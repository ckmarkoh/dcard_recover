{
  "name": "Dcard Recoverer",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Helps you recover the missing text.",
  "content_scripts": [
  	{
  		"matches": ["http://dcard.tw/*","http://www.dcard.tw/*"],
  		"js": ["jquery-1.8.3.min.js", "jquery-ui.js", "util2.js", "hack.js"],
  		"run_at": "document_end"
  	}
  ],
  "permissions": [
    "tabs", "http://dcard.tw/*","http://www.dcard.tw/*"
  ]
}